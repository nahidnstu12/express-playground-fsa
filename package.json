    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.0",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
